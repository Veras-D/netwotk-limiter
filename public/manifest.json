{
  "manifest_version": 3,
  "name": "Network Limiter",
  "version": "1.0",
  "description": "Limits network speed in DevTools.",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["debugger", "activeTab", "tabs", "storage"],
  "host_permissions": ["http://*/*", "https:/*/*"],
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}

